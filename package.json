{
    "name": "@weblif/fast-ui",
    "version": "0.29.3",
    "scripts": {
        "start": "dumi dev",
        "build": "tsc",
        "build:docs": "dumi build",
        "deploy": "npm run docs:build && npm run docs:deploy",
        "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,json,md}\"",
        "build:rm": "rimraf es",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "prepublishOnly": "npm run build:rm && npm run build"
    },
    "main": "es/index.js",
    "module": "es/index.js",
    "typings": "es/index.d.ts",
    "author": {
        "name": "苯苯",
        "email": "854363956@qq.com"
    },
    "keywords": [
        "react-component",
        "react",
        "UI"
    ],
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "files": [
        "es",
        "README.md"
    ],
    "license": "MIT",
    "homepage": "https://ui.lif.ink",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Web-Lif/fast-ui.git",
        "branch": "canary",
        "platform": "github"
    },
    "lint-staged": {
        "*.{js,jsx,less,md,json}": [
            "prettier --write"
        ],
        "*.ts?(x)": [
            "prettier --parser=typescript --write"
        ]
    },
    "devDependencies": {
        "@testing-library/react": "13.4.0",
        "@types/jest": "29.0.0",
        "@types/react-color": "3.0.6",
        "@weblif/react-top-loading-bar": "2.1.2",
        "core-js": "3.25.1",
        "cross-env": "7.0.3",
        "dumi": "1.1.47",
        "html-webpack-plugin": "4.5.2",
        "jest": "29.0.3",
        "jest-environment-jsdom": "29.0.3",
        "lint-staged": "13.0.3",
        "prettier": "2.7.1",
        "react-markdown": "8.0.3",
        "react-test-renderer": "18.2.0",
        "rimraf": "3.0.2",
        "ts-jest": "29.0.0",
        "ts-node-dev": "2.0.0",
        "yorkie": "2.0.0"
    },
    "peerDependencies": {
        "@ant-design/icons": "4.7.0",
        "@dnd-kit/core": "6.0.5",
