        "@weblif/rc-table": "0.7.1",
        "qrcode.react": "3.1.0",
        "react-color": "2.19.3",
        "react-spaces": "0.3.6"
    },
    "engines": {
        "node": ">=16.x",
        "npm": ">=8.x"
    },
    "sideEffects": false
}
